LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
ENTITY SELECAO_SAIDA IS
	PORT( S_SOM,S_SUB,Cout_SOM,Cout_SUB,S_LOGICA : IN BIT;
		  S,Cout : OUT BIT); 
END SELECAO_SAIDA;
ARCHITECTURE COMPORTAMENTAL OF SELECAO_SAIDA IS
BEGIN
	S <= (S_SOM OR S_SUB OR S_LOGICA);
	Cout <= (Cout_SOM OR Cout_SUB);
END COMPORTAMENTAL;